  pages = {1--20},
  url = {https://learn.streetlightdata.com/hubfs/White%20Papers/GHG%20Emissions%20Metrics/StreetLight%20GHG%20Emissions%20Methodology%20and%20Validation%20White%20Paper%20-%20September%202023.pdf},
