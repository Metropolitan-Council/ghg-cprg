@techreport{nrelBusinessasUsualElectricityNatural2019,
  type = {Government},
  title = {Business-as-{{Usual Electricity}} and {{Natural Gas Consumption}} and {{Expenditure Projections}}},
  author = {{NREL}},
  year = {2019},
  pages = {1--4},
  institution = {{NREL}},
  url = {https://app.box.com/s/t0t3j6zztzan94a4tu1so8pse4btkkn1},
  abstract = {Projected business-as-usual electricity and natural gas consumption is modeled for the residential, commercial, and industrial sectors for each city, county, and state using baseline 2016 estimates developed through the Cities Leading through Energy Analysis and Planning (Cities-LEAP) project (Ma et al 2019).1 The same general approach to project annual natural gas and electricity use and expenditures from 2017 to 2050 is used for the residential, commercial, and industrial sectors. The steps in the approach are summarized as a flow diagram in Figure 1. The data and their sources are summarized in Table 1. The first component of the approach projects an indicator of sector activity---number of occupied households for residential, building footprint (ft2) for commercial, and value of shipments (\$) for industry---using the annual rate of change estimated from time series models2 developed using historical data. The second component of the approach projects an indicator of sector energy efficiency---British thermal units (Btus) per number of occupied households for residential, Btus per building footprint (ft2) for commercial, and Btus per value of shipments (\$) for industry---and an energy price (in \$/Btu) based on sector efficiencies and energy prices calculated from 2016 Cities-LEAP data and the annual rate of change for sector efficiencies and energy prices from the Annual Energy Outlook (AEO) 2019. The third component of the approach calculates annual projected energy use by multiplying the activity indicator by the energy efficiency indicator; expenditures are calculated by multiplying energy by price. 1 Ma, Ookie, Ricardo Oliveira, Evan Rosenlieb, and Megan Day. 2019. Sector-Specific Methodologies for Subnational Energy Modeling. Golden, CO: National Renewable Energy Laboratory. NREL/TP-7A40-72748. https://www.nrel.gov/docs/fy19osti/72748.pdf. 2 Autoregressive integrated moving average (ARIMA) time series models are fit to historical data at the county or census tract level. Relevant historical activity data from the National Energy Modeling System (NEMS) are included as an exogenous variable to tie ARMIA model projections to AEO 2019 projections.},
  langid = {english}
}

