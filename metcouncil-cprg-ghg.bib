@techreport{streetlightdataStreetLightAllVehicles2023,
  type = {Industry},
  title = {{{StreetLight All Vehicles Volume Methodology}} and {{Validation White Paper}} - {{United States}}},
  shorttitle = {{{StreetLight All Vehicles Volume}}},
